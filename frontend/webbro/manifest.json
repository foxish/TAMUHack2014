{
  "name": "WebBro",
  "version": "1.00",
  "manifest_version": 2,
  "description": "Crowd-sourced commentary for the web",
  "browser_action": {
    "default_icon": "images/icon.png",
	"default_popup": "popup.html"
  },
  
  "permissions": [
	"<all_urls>",
	"tabs",
	"storage"
  ],
  "icons": { 
            "32": "images/icon32.png",
            "48": "images/icon48.png",
            "128": "images/icon.png" 
   }
}
